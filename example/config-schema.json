{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "$schema"
  ],
  "propertyNames": {
    "pattern": "^([0-9a-zA-Z_/-]{44})|\\$schema$",
    "title": "A spreadsheet to load"
  },
  "properties": {
    "$schema": {
      "type": "string",
      "default": ""
    }
  },
  "patternProperties": {
    "^[0-9a-zA-Z_/-]{44}$": {
      "type": "object",
      "title": "A sheet to load",
      "patternProperties": {
        ".": {
          "type": "string",
          "pattern": "^[a-zA-Z0-9_-]+$",
          "title": "The table name to load this sheet as"
        }
      }
    }
  }
}